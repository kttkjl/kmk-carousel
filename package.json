{
  "name": "kmk-carousel",
  "version": "1.1.11",
  "description": "Simple infinite scroll carousel",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
